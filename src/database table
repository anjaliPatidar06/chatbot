CREATE TABLE slot (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, slot_name NVARCHAR(80) NOT NULL,slot_type NVARCHAR(200) NOT NULL,slot_value NVARCHAR(200) NULL, chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE Stories (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, name NVARCHAR(180) NOT NULL, story NVARCHAR(380) NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE companyMaster (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, companyname NVARCHAR(80) NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE userMaster1 (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, userRole NVARCHAR(80) NOT NULL,name NVARCHAR(80) NOT NULL,email NVARCHAR(50) NOT NULL ,phone NVARCHAR(50) NOT NULL, password NVARCHAR(20) NOT NULL, category NVARCHAR(50) NULL,country NVARCHAR(80) NOT NULL, date_creation datetime,Date_modify datetime,companyID INT NULL,star_date datetime,end_date datetime,subscription_id INT NULL)
CREATE TABLE NLU_intent (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, intent NVARCHAR(80) NOT NULL, chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE NLU_sentence (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, sentence NVARCHAR(80) NOT NULL,intent_id INT NOT NULL,Entity NVARCHAR(180) NOT NULL , chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE slot (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, slot_name NVARCHAR(80) NOT NULL,slot_type NVARCHAR(200) NOT NULL, chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE form (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, Form_name NVARCHAR(180) NOT NULL, chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE Response (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, response_name NVARCHAR(180) NOT NULL,text NVARCHAR(300) NOT NULL, chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE QuickReplies (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, title NVARCHAR(180) NOT NULL,payload NVARCHAR(300) NOT NULL, entity_key NVARCHAR(300) NOT NULL,entity_value NVARCHAR(300) NOT NULL, response_id INT NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE dropdown (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, label NVARCHAR(180) NOT NULL,value NVARCHAR(300) NOT NULL, entity_key NVARCHAR(300) NOT NULL,entity_value NVARCHAR(300) NOT NULL, response_id INT NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE Button (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, title NVARCHAR(180) NOT NULL,payload NVARCHAR(300) NOT NULL, entity_key NVARCHAR(300) NOT NULL,entity_value NVARCHAR(300) NOT NULL, response_id INT NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE cardcorcousel (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, card_title NVARCHAR(180) NOT NULL,image_path NVARCHAR(300) NOT NULL,payload NVARCHAR(300) NOT NULL, entity_key NVARCHAR(300) NOT NULL,entity_value NVARCHAR(300) NOT NULL, response_id INT NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE action (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, action_name NVARCHAR(180) NOT NULL, chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE Regex (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, Regex_name NVARCHAR(180) NOT NULL, Regex_pattern NVARCHAR(380) NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE synonym (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, synonym_key NVARCHAR(180) NOT NULL, synonym_value NVARCHAR(380) NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE Entity (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, Entity_key NVARCHAR(180) NOT NULL, Entity_value NVARCHAR(380) NOT NULL,sentence_id INT NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE cards (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, card_title NVARCHAR(180) NOT NULL,image_path NVARCHAR(300) NULL,payload NVARCHAR(300) NOT NULL,description NVARCHAR(300) NULL,title NVARCHAR(300) NOT NULL, entity_key NVARCHAR(300) NOT NULL,entity_value NVARCHAR(300) NOT NULL, response_id INT NOT NULL,chatbot_id INT NOT NULL, date_creation datetime,Date_modify datetime)
CREATE TABLE Subscription (id INT IDENTITY (1,1) NOT NULL PRIMARY KEY, subname NVARCHAR(80) NOT NULL,subcost NVARCHAR(80) NOT NULL,currency NVARCHAR(80) NOT NULL,message NVARCHAR(50) NOT NULL,duration NVARCHAR(50) NOT NULL, botcustomization NVARCHAR(20) NOT NULL,pretrained NVARCHAR(120) NOT NULL ,removable NVARCHAR(120) NOT NULL ,engagement NVARCHAR(120) NOT NULL,priority NVARCHAR(120) NOT NULL, schedule NVARCHAR(120) NOT NULL,livechat NVARCHAR(120) NOT NULL,helpnew NVARCHAR(120) NOT NULL,priority NVARCHAR(120) NOT NULL,remote NVARCHAR(120) NOT NULL,channel NVARCHAR(120) NOT NULL, conversation NVARCHAR(120) NOT NULL, sales NVARCHAR(120) NOT NULL,multiple NVARCHAR(120) NOT NULL, date_creation datetime,date_modify datetime,new_date NVARCHAR(80)  NULL, nuser  INT NULL, companyID INT NULL,chatbotid INT NULL)

